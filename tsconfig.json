{
  "compilerOptions": {
    "target": "esNext",
    "module": "ES2015",
    "jsx": "react",
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "src/*": [
          "src/*"
      ]
  },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src/**/*"
  ]
}